#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/flavionn/Documentos/www/estudo/2022-franq/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/flavionn/Documentos/www/estudo/2022-franq/node_modules/.pnpm/node_modules"
fi
"$basedir/../../../../../esbuild@0.14.42/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
